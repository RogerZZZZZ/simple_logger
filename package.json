{
  "name": "logger",
  "version": "0.0.1",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "webpack-dashboard -- webpack-dev-server --config ./webpack.config.js --hot --inline --progress --watch --open",
    "server:prod": "cross-env NODE_ENV=production webpack-dashboard -- webpack-dev-server --config ./webpack.config.js --port 3001 --host 0.0.0.0 --hot --inline --progress --profile --watch --open --content-base dist/",
    "build": "npm run build:prod",
    "build:dev": "webpack --config ./webpack.config.js --progress --profile --color --display-error-details --display-cached",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack.config.js  --progress --profile --color --display-error-details --display-cached --bail",
    "clean": "npm cache clear && rimraf -- dist",
    "test": "karma start"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jasmine": "^2.8.16",
    "@types/lodash": "^4.14.121",
    "awesome-typescript-loader": "^3.2.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.4",
    "ejs-loader": "^0.3.0",
    "eslint": "^4.2.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine": "^2.6.0",
    "jasmine-core": "^2.6.4",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.4",
    "rimraf": "^2.6.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.18.2",
    "tslint": "^5.12.1",
    "tslint-loader": "^3.5.4",
    "typescript": "^2.9.2",
    "webpack": "^3.3.0",
    "webpack-dashboard": "^2.1.0",
    "webpack-dev-server": "^3.1.11"
  },
  "dependencies": {
    "@types/platform": "^1.3.1",
    "axios": "^0.18.0",
    "platform": "^1.3.5"
  }
}
